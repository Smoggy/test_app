<h1> All products </h1>


<% if user_signed_in? %>
  <%= link_to("Add", new_product_path, remote: true,  class: "btn btn-primary") %>
<% end %>
<div class="new-product"></div>


<%= will_paginate %>

<table class="table table-striped">
      <thead>
        <tr>
          <th>Title</th>
          <th>Description</th>
          <th>Owner</th>
          <th>Lat/Long</th>
          <th>-</th>
        </tr>
      </thead>
      <tbody class="product-index" >

        <%= render "index" %>

      </tbody>
    </table>




<%= will_paginate %>
<div id="product-modal" class="modal fade">
  <div class="modal-dialog">
    <div id="inner-product-modal" class="modal-content"></div>
  </div>
</div>